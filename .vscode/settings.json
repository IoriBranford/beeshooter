{
    "files.associations": {
        "*.lua": "lua",
        "*.h": "c",
        "*.c": "c"
    },
    "Lua.workspace.library": [
        "${3rd}/love2d/library"
    ],
    "Lua.workspace.checkThirdParty": false,
    "Lua.runtime.path": [
        "game/?.lua",
        "game/?/init.lua",
        "game/libraries/?.lua",
        "game/libraries/?/init.lua",
        "game/code/?.lua",
        "game/code/?/init.lua"
    ],
    "Lua.diagnostics.globals": [
        "class"
    ],
    "Lua.runtime.version": "LuaJIT",
    "workbench.colorTheme": "Bee Theme"
}